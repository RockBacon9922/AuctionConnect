    "react-redux": "^8.1.1",
    "redux-persist-webextension-storage": "^1.0.2",
    "redux-thunk": "^2.4.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@acme/tailwind-config": "^0.1.0",
    "@types/chrome": "0.0.210",
